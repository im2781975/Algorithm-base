#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n=4,i,j;
    vector<int>adj_list[n];
    adj_list[0]={1};
    adj_list[1]={0,2,3};
    adj_list[2]={1,3};
    adj_list[3]={1,2};
    for(i=0;i<n;i++)
    {
        cout<<i<<"->";
        for(j=0;j<adj_list[i].size();j++)
        cout<<adj_list[i][j]<<" ";
        
        cout<<"\n";
    }
}
