/*1 2 0
2 4 0
2 8 0
1 7 0 */
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n=4,i,j;
    vector<vector<int>>edge_list;
    edge_list.push_back({1,2,0});
    edge_list.push_back({2,4,0});
    edge_list.push_back({2,8,0});
    edge_list.push_back({1,7,0});
    for(i=0;i<edge_list.size();i++)
    {
        cout<<edge_list[i][0]<<" "<<edge_list[i][1]<<" "<<edge_list[i][2];
        cout<<"\n";
    }
}
