#include <bits/stdc++.h>
using namespace std;

class node {
public:
    int data;
    node* nxt;
    node* prv;
    node(int data) {
        this->data = data;
        nxt = NULL;
        prv = NULL;
    }
};

class MyDeque {
public:
    int size;
    node* head;
    node* tail;
    MyDeque() {
        size = 0;
        head = NULL;
        tail = NULL;
    }
    void push_back(int val) {
        node* newnode = new node(val);
        if (size == 0) {
            head = newnode;
            tail = newnode;
            size++;
        }
        tail->nxt = newnode;
        newnode->prv = tail;
        tail = newnode;
        size++;
    }
    int get_size() {
        return size;
    }
    void pop_back()
    {
        if(size==0)
        {
            cout<<"nothing"<<"\n";
            return;
        }
        if(head==tail)
        {
            node* tmp=head;
            head=NULL;
            tail=NULL;
            delete tmp;
            size--;
        }
        node* delnode=tail;
        tail=delnode->prv;
        delete delnode;
        size--;
    }
};

int main() {
    MyDeque dq;
    //dq.push_back(10);
    dq.push_back(10);
    dq.push_back(10);
    dq.push_back(10);
    cout << dq.get_size();
    return 0;
}

