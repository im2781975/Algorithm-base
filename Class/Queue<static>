#include<bits/stdc++.h>
using namespace std;
class StaticQ{
    int arr[100], frontIdx, rareIdx;
    public:
    StaticQ(){
        frontIdx = -1; 
        rareIdx = -1;
    }
    bool IsEmpty(){
        return frontIdx == -1;
    }
    bool IsFull(){
        return rareIdx == 99;
    }
    void enqueue(int item){
        if(IsFull())
            cout << "\nQueue is full";
        else{
            if(IsEmpty())
                frontIdx = 0;
            arr[++rareIdx] = item;
        }
    }
    void dequeue(){
        if(IsEmpty())
            cout << "queue is empty";
        else{
            if(frontIdx == rareIdx)
                frontIdx = rareIdx = -1;
            else
                frontIdx++;
        }
    }
    int getfront(){
        if(IsEmpty()){
            cout << "Queue is empty";
            return -1;
        }
        else
            return arr[frontIdx];
    }
};
int main(){
    StaticQ q;
    q.enqueue(2);
    q.enqueue(5);
    q.enqueue(8);
    cout << "Front: " << q.getfront();
}
